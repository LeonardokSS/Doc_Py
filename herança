
class Pessoa():

    

    def __init__(self):
        self.nome = 'Roberto'
        self.idade = '16'
        self.gênero = 'Masculino'
        self.telefone = '11978421029'

    def get_nome(self):
        return self.nome 
    
    def set_nome(self,nome):
        self.nome = nome

    def get_idade(self):
        return self.idade 
    
    def set_idade(self,idade):
        self.idade = idade
    
    def get_gênero(self):
        return self.gênero 
    
    def set_gênero(self,gênero):
        self.gênero = gênero

    
    def get_telefone(self):
        return self.telefone 
    
    def set_telefone(self,telefone):
        self.telefone = telefone


Leonardo = Pessoa()

Leonardo.set_gênero = 'Masculino'
Leonardo.set_numero = '1198098981'
Leonardo.set_nome = 'Leonardo'
Leonardo.set_idade = '23'

class Aluno(Pessoa)
    def __init__()
    self.matricula = '1'

